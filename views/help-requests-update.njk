{% from 'lbh-button/macro.njk' import lbhButton %}
{% from 'lbh-panel/macro.njk' import lbhPanel %}

{% extends "./layouts/base.njk" %}

{% block content %}
    <h1 class="lbh-heading-h1">Coronavirus Get Help Admin</h1>
        <h2 class="lbh-heading-h2"></h2>

    {{ lbhPanel({
    "titleHtml": "UPRN: " + Uprn + " Annex ID: " + Id,
    "text": "Update successful"
    }) }}

    <p class="lbh-body-m">{{updatedData.OngoingFoodNeed}} <br />
    {{updatedData.NumberOfPeopleInHouse}} <br />
    {{updatedData.LastConfirmedFoodDelivery}}</p>

    {{ lbhButton({
        "text": "Home",
        href: "/"
    })}}
{% endblock %}