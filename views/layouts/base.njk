{% extends "template.njk" %}
{% from "lbh-back-to-top/macro.njk" import lbhBackToTop %}

{% block head %}
   <link rel="stylesheet" type="text/css" href="/app.css">
{% endblock %}


{% block pageTitle %}Coronavirus - I Need Help Admin | Hackney Council{% endblock %}

{% block header %}
  {{lbhHeader({
    isHome: false,
    serviceName: "I Need Help Admin"
  })}}
{% endblock %}

{% block beforeContent %}
<div id="loader_overlay" class="">
  <div id="loader_spinner"></div>
</div>
{% endblock %}

{% block bodyEnd %}
